<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="calculator" class="com.tj.cal_ex.Calculator"></bean>
	<bean id="mycal" class="com.tj.cal_ex.MyCal">
		<property name="cal" ref="calculator"></property>
		<property name="sn" value="20"></property>
		<property name="fn" value="10"></property>
	</bean>

</beans>
